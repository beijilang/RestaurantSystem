<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="frontend.GUI.PrintBillController">
    <children>
        <Label fx:id="tableNumberLabel" layoutX="23.0" layoutY="38.0" text="Print Bill for Table: (null) ">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label fx:id="billToString" alignment="TOP_LEFT" layoutX="14.0" layoutY="71.0" prefHeight="145.0"
               prefWidth="372.0" text="(Click button on how to print bill.)"/>
        <Button fx:id="oneBill" layoutX="418.0" layoutY="541.0" mnemonicParsing="false" prefHeight="27.0"
                prefWidth="69.0" text="One bill"/>
        <Button fx:id="splitBill" layoutX="497.0" layoutY="541.0" mnemonicParsing="false" prefHeight="27.0"
                prefWidth="75.0" text="Split bill"/>

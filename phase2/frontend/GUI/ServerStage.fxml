<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<SplitPane dividerPositions="0.2993311036789298" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.GUI.ServerController">
    <VBox layoutX="10.0" layoutY="25.0" prefHeight="398.0" prefWidth="175.0" spacing="5.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0" SplitPane.resizableWithParent="false">
        <padding>
            <Insets left="5.0" right="5.0" top="5.0"/>
        </padding>
        <StackPane onMouseClicked="#takeSeat">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Take Seat"/>
        </StackPane>
        <StackPane onMouseClicked="#order">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Order Food"/>
        </StackPane>
        <StackPane onMouseClicked="#bill">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Print Bill"/>
        </StackPane>
        <StackPane onMouseClicked="#deliverSuccessful">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Deliver Successful"/>
        </StackPane>
        <StackPane onMouseClicked="#deliverFailed">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Deliver Failed"/>
        </StackPane>
        <StackPane onMouseClicked="#clear">
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3d3d3" height="47.0" stroke="#806e6e"
                       strokeType="INSIDE" width="175.0">
                <VBox.margin>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="20.0"/>
                </VBox.margin>
            </Rectangle>
            <Label text="Clear Table"/>
        </StackPane>
    </VBox>
    <VBox>
        <VBox fx:id="gridParent" minHeight="0.0" minWidth="0.0" prefHeight="554.0" prefWidth="405.0">
            <HBox alignment="TOP_RIGHT" prefHeight="31.0" prefWidth="405.0" spacing="3.0">
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
                <padding>
                    <Insets right="3.0" top="3.0"/>
                </padding>
                <Button mnemonicParsing="false" onAction="#receiveItem" text="Receive item">
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#logOff" text="Sign out">
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Button>
            </HBox>
            <HBox fx:id="hBox1" prefHeight="25.0" prefWidth="405.0"/>
            <HBox fx:id="hBox2" prefHeight="25.0" prefWidth="405.0"/>
        </VBox>
        <Pane prefHeight="328.0" prefWidth="405.0">
            <TableView prefHeight="200.0" prefWidth="405.0" fx:id="finishedDishTableView">
                <columns>
                    <TableColumn prefWidth="156.0" text="Finished dish name">
                        <cellValueFactory>
                            <PropertyValueFactory property="name"/>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn minWidth="0.0" prefWidth="102.0" text="dish number">
                        <cellValueFactory>
                            <PropertyValueFactory property="dishNumber"/>
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn maxWidth="146.0" minWidth="0.0" prefWidth="146.0" text="table number">
                        <cellValueFactory>
                            <PropertyValueFactory property="tableNumber"/>
                        </cellValueFactory>
                    </TableColumn>
                </columns>
                <padding>
                    <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                </padding>
            </TableView>
        </Pane>
    </VBox>
</SplitPane>
